class WhileDoWhile 
{
	public static void main(String[] args) 
	{
		
		int j =100;
		do
		{
			System.out.println("Hello World!");
			j++;
		}	
		while (j<=3);
		

	System.out.println("------------------------------");
		int i=100;
		while(i<=3)
		{
			System.out.println("Hello World!");
			i++;
		}
	}
}
