<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="methodlog">  
  <insert id="insert">
  	insert into methodlog values(seq_methodlog.nextval, #{methodname},sysdate,#{requiredtime})
  </insert>
</mapper>